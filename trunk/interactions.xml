<!-- interaction name means a common name, type means what sort of geometry shd be checked-->
<!-- group type is either A or D, must be marked to avoid detection of AA or DD spurious detections-->

<!-- ***************************DHR start********************************** -->
<!-- pi hydrogen bond CaH..pi -->
<interaction name="CaHpi" type="DHR">
<group AD="D" res="AA"><atom tag="D" atn=" CA "></atom></group>
<group AD="A" res="PHE"><atom tag="R" atn=" CG "></atom><atom tag="R" atn=" CD1"></atom><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE1"></atom><atom tag="R" atn=" CE2"></atom><atom tag="R" atn=" CZ "></atom></group>
<group AD="A" res="TYR"><atom tag="R" atn=" CG "></atom><atom tag="R" atn=" CD1"></atom><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE1"></atom><atom tag="R" atn=" CE2"></atom><atom tag="R" atn=" CZ "></atom></group>
<group AD="A" res="TRP"><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE2"></atom><atom tag="R" atn=" CE3"></atom><atom tag="R" atn=" CZ2"></atom><atom tag="R" atn=" CZ3"></atom><atom tag="R" atn=" CH2"></atom></group>
<group AD="A" res="TRP"><atom tag="R" atn=" CG "></atom><atom tag="R" atn=" CD1"></atom><atom tag="R" atn=" NE1"></atom><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE2"></atom></group>
<lim tag1="D" tag2="Am" min="2" max="4.3"></lim>
<lim tag1="H" tag2="Am" min="2" max="3.8"></lim>
<lim tag1="D" tag2="H" tag3="Am" min="120" max="180"></lim>
<lim tag1="D" tag2="Am" tag3="An" min="0" max="30"></lim>
</interaction>

<!-- pi hydrogen bond NH..pi -->
<interaction name="NHpi" type="DHR">
<group AD="D" res="AA"><atom tag="D" atn=" N  "></atom></group>
<group AD="A" res="PHE"><atom tag="R" atn=" CG "></atom><atom tag="R" atn=" CD1"></atom><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE1"></atom><atom tag="R" atn=" CE2"></atom><atom tag="R" atn=" CZ "></atom></group>
<group AD="A" res="TYR"><atom tag="R" atn=" CG "></atom><atom tag="R" atn=" CD1"></atom><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE1"></atom><atom tag="R" atn=" CE2"></atom><atom tag="R" atn=" CZ "></atom></group>
<group AD="A" res="TRP"><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE2"></atom><atom tag="R" atn=" CE3"></atom><atom tag="R" atn=" CZ2"></atom><atom tag="R" atn=" CZ3"></atom><atom tag="R" atn=" CH2"></atom></group>
<group AD="A" res="TRP"><atom tag="R" atn=" CG "></atom><atom tag="R" atn=" CD1"></atom><atom tag="R" atn=" NE1"></atom><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE2"></atom></group>
<lim tag1="D" tag2="Am" min="2" max="4.3"></lim>
<lim tag1="H" tag2="Am" min="2" max="3.5"></lim>
<lim tag1="D" tag2="H" tag3="Am" min="120" max="180"></lim>
<lim tag1="D" tag2="Am" tag3="An" min="0" max="30"></lim>
</interaction>


<!-- cation-pi -->
<interaction name="cationPi" type="DHR">
<group AD="D" res="LYS"><atom tag="D" atn=" NZ "></atom></group>
<group AD="D" res="ARG"><atom tag="D" atn=" NH1"></atom></group>
<group AD="D" res="ARG"><atom tag="D" atn=" NH2"></atom></group>
<group AD="D" res="ARG"><atom tag="D" atn=" NE "></atom></group>
<group AD="A" res="PHE"><atom tag="R" atn=" CG "></atom><atom tag="R" atn=" CD1"></atom><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE1"></atom><atom tag="R" atn=" CE2"></atom><atom tag="R" atn=" CZ "></atom></group>
<group AD="A" res="TYR"><atom tag="R" atn=" CG "></atom><atom tag="R" atn=" CD1"></atom><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE1"></atom><atom tag="R" atn=" CE2"></atom><atom tag="R" atn=" CZ "></atom></group>
<group AD="A" res="TRP"><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE2"></atom><atom tag="R" atn=" CE3"></atom><atom tag="R" atn=" CZ2"></atom><atom tag="R" atn=" CZ3"></atom><atom tag="R" atn=" CH2"></atom></group>
