<lim tag1="D" tag2="Am" min="2" max="4.0"></lim>
<lim tag1="H" tag2="Am" min="2" max="3.8"></lim>
<lim tag1="D" tag2="H" tag3="Am" min="10" max="180"></lim>
<lim tag1="D" tag2="Am" tag3="An" min="0" max="30"></lim>
</interaction>

<!-- cysteine - pi -->
<interaction name="thiolPi" type="DHR">
<group AD="D" res="CYS"><atom tag="D" atn=" SG "></atom></group>
<group AD="A" res="PHE"><atom tag="R" atn=" CG "></atom><atom tag="R" atn=" CD1"></atom><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE1"></atom><atom tag="R" atn=" CE2"></atom><atom tag="R" atn=" CZ "></atom></group>
<group AD="A" res="TYR"><atom tag="R" atn=" CG "></atom><atom tag="R" atn=" CD1"></atom><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE1"></atom><atom tag="R" atn=" CE2"></atom><atom tag="R" atn=" CZ "></atom></group>
<group AD="A" res="TRP"><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE2"></atom><atom tag="R" atn=" CE3"></atom><atom tag="R" atn=" CZ2"></atom><atom tag="R" atn=" CZ3"></atom><atom tag="R" atn=" CH2"></atom></group>
<group AD="A" res="TRP"><atom tag="R" atn=" CG "></atom><atom tag="R" atn=" CD1"></atom><atom tag="R" atn=" NE1"></atom><atom tag="R" atn=" CD2"></atom><atom tag="R" atn=" CE2"></atom></group>
